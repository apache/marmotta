<#TCC9> a td:TestCase;
    rdfs:label "TC-C9";
    tn:testAssertion <#TCC9-A1-Response-1-GET>,
        <#TCC9-A2-Response-1-GET>,
        <#TCC9-A3-Response-1-GET>,
        <#TCC9-A4-Response-2-POST>,
        <#TCC9-A5-Response-3-GET>,
